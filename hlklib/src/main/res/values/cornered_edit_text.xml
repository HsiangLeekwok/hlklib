<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CorneredEditText">
        <attr name="cet_active_border" format="color" />
        <attr name="cet_normal_border" format="color" />
        <attr name="cet_disabled_border" format="color" />
        <attr name="cet_corner_size" format="dimension" />
        <attr name="cet_left_top_corner" format="dimension" />
        <attr name="cet_left_bottom_corner" format="dimension" />
        <attr name="cet_right_top_corner" format="dimension" />
        <attr name="cet_right_bottom_corner" format="dimension" />
