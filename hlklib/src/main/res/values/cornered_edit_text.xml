        <attr name="cet_border_size" format="dimension" />
        <attr name="cet_value_verify_regex" format="string" />
        <attr name="cet_value_extract_regex" format="string" />
        <attr name="cet_value_verify_warning" format="string" />
        <attr name="cet_display_counter" format="boolean" />
        <attr name="cet_counter_font_size" format="dimension" />
